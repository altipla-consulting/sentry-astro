---
import { getTraceMetaTags } from '@sentry/node'
import { sentryOptions } from 'virtual:@altipla/sentry-astro/config'
import { generateOptions } from './options'
---

<Fragment set:html={getTraceMetaTags()} />
<script define:vars={{ sentry: generateOptions(sentryOptions) }}>
  window.__sentry = sentry
</script>
